{
  "name": "server",
  "version": "1.0.0",
  "private": true,
  "description": "Serveur SocketIO de base",
  "scripts": {
    "start": "nodemon",
    "build": "rimraf out && tsc --project tsconfig.app.json",
    "test": "cross-env TS_NODE_PROJECT='./tsconfig.spec.json' mocha --config=.mocharc.json --node-env=test",
    "coverage": "cross-env TS_NODE_PROJECT='./tsconfig.spec.json' nyc mocha --config=.mocharc.json --node-env=test --exit"
  },
  "author": "Nikolay Radoev",
  "license": "ISC",
  "nyc": {
    "check-coverage": true,
    "lines": 50,
    "per-file": true,
    "include": [
      "app/**/*.ts"
    ],
    "extension": [
      ".ts"
    ],
    "exclude": [
      "**/*.spec.ts",
      "app/index.ts",
      "app/server.ts",
      "app/app.ts",
      "app/classes/http.exception.ts"
    ],
    "require": [
      "ts-node/register",
      "tsconfig-paths/register"
    ],
    "reporter": [
      "html",
      "text",
      "cobertura"
    ],
    "all": true,
    "cache": true,
    "sourceMap": true,
    "instrument": true
  },
  "_moduleAliases": {
    "@app": "out/"
  },
  "dependencies": {
    "cookie-parser": "1.4.5",
    "cors": "2.8.5",
    "express": "4.18.1",
    "http-status-codes": "2.1.4",
    "module-alias": "2.2.2",
    "reflect-metadata": "0.1.13",
    "socket.io": "4.5.1",
    "socket.io-client": "4.5.1",
    "typedi": "0.10.0"
  },
  "devDependencies": {
    "@types/body-parser": "1.19.1",
    "@types/chai": "4.3.0",
    "@types/cookie-parser": "1.4.3",
    "@types/cors": "2.8.12",
    "@types/express": "4.17.13",
    "@types/mocha": "9.1.1",
    "@types/node": "18.6.5",
    "@types/sinon": "10.0.13",
    "chai": "4.3.4",
    "cross-env": "7.0.3",
    "mocha": "10.0.0",
    "nodemon": "2.0.19",
    "nyc": "15.1.0",
    "rimraf": "3.0.2",
    "sinon": "11.1.2",
    "source-map-support": "0.5.19",
    "ts-node": "10.9.1",
    "tsconfig-paths": "4.1.0",
    "typescript": "4.7.4"
  }
}
